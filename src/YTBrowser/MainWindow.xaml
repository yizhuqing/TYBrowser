<Window x:Class="TYBrowser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TYBrowser"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded" WindowStyle="None" WindowStartupLocation="CenterScreen" WindowState="Maximized" KeyDown="Window_KeyDown" Icon="logo.ico">

    <GroupBox x:Name="gSetting" Header="启动参数" VerticalAlignment="Top" HorizontalAlignment="Center" Grid.IsSharedSizeScope="True" Width="782" FontSize="20">
        <Grid>
            <Label Content="   系统地址：" VerticalAlignment="Top" FontSize="18" Margin="43,0,608,0"/>
            <TextBox x:Name="txtUrl" HorizontalAlignment="Right" Height="35" TextWrapping="Wrap" VerticalAlignment="Top" Width="455" RenderTransformOrigin="0.5,0.5" FontSize="18" Margin="0,-1,148,0">

            </TextBox>
            <Button x:Name="btnLoad" Content="加载" VerticalAlignment="Top" Click="btnLoad_Click" HorizontalAlignment="Right" Width="75" FontSize="18" RenderTransformOrigin="0.413,1.628" Margin="0,-1,33,0"/>

        </Grid>
    </GroupBox>
</Window>
